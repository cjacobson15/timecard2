<div class="page-header">
  <h3>Admin #<%= @admin.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Full access</dt>
      <dd><%= @admin.full_access %></dd>

      <dt>User</dt>
      <dd><%= @admin.user_id %></dd>

      <dt>Company</dt>
      <dd><%= @admin.company_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/admins" class="btn btn-primary">
        Back
      </a>
      <a href="/admins/<%= @admin.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_admin/<%= @admin.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
